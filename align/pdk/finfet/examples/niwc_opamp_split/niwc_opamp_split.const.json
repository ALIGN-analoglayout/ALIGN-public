[
    {"constraint": "ConfigureCompiler", "auto_constraint": false, "merge_parallel_devices": false},
    {"constraint": "PowerPorts", "ports": ["vccx"]},
    {"constraint": "GroundPorts", "ports": ["vssx"]},
    {"constraint": "GroupBlocks", "instances": ["m1", "m2"], "instance_name": "xdp", "generator": { "instance_name": "MOS", "parameters": { "style": "RADHARD"}}},
    {"constraint": "GroupBlocks", "instances": ["m7a", "m8a"], "instance_name": "xnraila", "generator": { "instance_name": "MOS", "parameters": { "style": "RADHARD"}}},

    {"constraint": "GroupBlocks", "instances": ["m7b", "m8b"], "instance_name": "xnrailb", "generator": { "instance_name": "MOS", "parameters": { "style": "RADHARD"}}},
    {"constraint": "GroupBlocks", "instances": ["m11", "m12"], "instance_name": "xprail", "generator": { "instance_name": "MOS", "parameters": { "style": "RADHARD"}}},
    {"constraint": "GroupBlocks", "instances": ["m3a", "m4a"], "instance_name": "xlsa", "generator": { "instance_name": "MOS"}},
    {"constraint": "GroupBlocks", "instances": ["m3b", "m4b"], "instance_name": "xlsb", "generator": { "instance_name": "MOS"}},
    {"constraint": "GroupBlocks", "instances": ["m5a", "m6a"], "instance_name": "xostagea", "generator": { "instance_name": "MOS"}},
    {"constraint": "GroupBlocks", "instances": ["m5b", "m6b"], "instance_name": "xostageb", "generator": { "instance_name": "MOS"}},
    {"constraint": "SameTemplate", "instances": ["xlsa", "xlsb"]},
    {"constraint": "SameTemplate", "instances": ["xostagea", "xostageb"]},
    {"constraint": "SameTemplate", "instances": ["xnraila", "xnrailb"]},
    {"constraint": "Floorplan",
     "order": true,
     "symmetrize": true,
     "regions": [
        ["xprail"],
        ["xostagea", "xlsa", "xdp", "xlsb", "xostageb"],
        ["xnraila","mtail", "xnrailb"]
     ]},
    {"constraint": "MultiConnection", "nets": ["tail"], "multiplier": 4}
]
