
alias measurement get_dc {
  run dcOp
	export real dv_M0 = I0.m0:vds - (I0.m0:vgs - I0.m0:vth)
	export real dv_M1 = I0.m1:vds - (I0.m1:vgs - I0.m1:vth)
	export real dv_M2 = I0.m2:vds - (I0.m2:vgs - I0.m2:vth)
	export real dv_M3 = I0.m3:vds - (I0.m3:vgs - I0.m3:vth)
	export real dv_M4 = abs(I0.m4:vds) - (abs(I0.m4:vgs) - abs(I0.m4:vth))
	export real dv_M5 = abs(I0.m5:vds) - (abs(I0.m5:vgs) - abs(I0.m5:vth))
	export real ov_M0 = I0.m0:vgs - I0.m0:vth
	export real ov_M1 = I0.m1:vgs - I0.m1:vth
	export real ov_M2 = I0.m2:vgs - I0.m2:vth
	export real ov_M3 = I0.m3:vgs - I0.m3:vth
	export real ov_M4 = abs(I0.m4:vgs) - abs(I0.m4:vth)
	export real ov_M5 = abs(I0.m5:vgs) - abs(I0.m5:vth)
}


foreach data{
	foreach vcm_r from swp(start=0.565, stop=0.835, step=0.27){
				run get_dc
	}
}
