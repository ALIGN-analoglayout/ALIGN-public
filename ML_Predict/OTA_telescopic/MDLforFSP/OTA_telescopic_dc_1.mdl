
alias measurement get_dc {
  run dcOp
	export real dv_M01 = I1.M01:vds - (I1.M01:vgs - I1.M01:vth)
	export real dv_M02 = I1.M02:vds - (I1.M02:vgs - I1.M02:vth)
	export real dv_M03 = I1.M03:vds - (I1.M03:vgs - I1.M03:vth)
	export real dv_M04 = I1.M04:vds - (I1.M04:vgs - I1.M04:vth)
  export real dv_M05 = I1.M05:vds - (I1.M05:vgs - I1.M05:vth)
	export real dv_M06 = I1.M06:vds - (I1.M06:vgs - I1.M06:vth)
  export real dv_M07 = abs(I1.M07:vds) - (abs(I1.M07:vgs) - abs(I1.M07:vth))
	export real dv_M08 = abs(I1.M08:vds) - (abs(I1.M08:vgs) - abs(I1.M08:vth))
  export real dv_M09 = abs(I1.M09:vds) - (abs(I1.M09:vgs) - abs(I1.M09:vth))
	export real dv_M10 = abs(I1.M10:vds) - (abs(I1.M10:vgs) - abs(I1.M10:vth))
	export real ov_M01 = I1.M01:vgs - I1.M01:vth
	export real ov_M02 = I1.M02:vgs - I1.M02:vth
	export real ov_M03 = I1.M03:vgs - I1.M03:vth
	export real ov_M04 = I1.M04:vgs - I1.M04:vth
  export real ov_M05 = I1.M05:vgs - I1.M05:vth
	export real ov_M06 = I1.M06:vgs - I1.M06:vth
	export real ov_M07 = abs(I1.M07:vgs) - abs(I1.M07:vth)
	export real ov_M08 = abs(I1.M08:vgs) - abs(I1.M08:vth)
  export real ov_M09 = abs(I1.M09:vgs) - abs(I1.M09:vth)
  export real ov_M10 = abs(I1.M10:vgs) - abs(I1.M10:vth)
}


foreach rpnet0239 from swp(start=0, stop=40, step=0.1){
	foreach vcm_r from swp(start=0.41, stop=0.59, step=0.18){
				run get_dc
	}
}
