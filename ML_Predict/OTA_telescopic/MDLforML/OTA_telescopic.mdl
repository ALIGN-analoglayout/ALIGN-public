

alias measurement get_ac{

// Maintain alphebatical order here.
	run ac
	export real bwout = bw(mag(V(n6)),response='low)
	export real gainbwprod = gainBwProd(mag(V(n6)))
//	export real gm = gainmargin(V(n6))
	export real maxgain = db(max(yval(mag(V(n6)))))
	export real pm = phasemargin(V(n6))


}

foreach data {
	run get_ac
}
