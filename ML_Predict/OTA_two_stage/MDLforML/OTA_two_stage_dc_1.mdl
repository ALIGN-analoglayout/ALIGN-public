
alias measurement get_dc {
  run dcOp
	export real dv_M0 = I0.M0:vds - (I0.M0:vgs - I0.M0:vth)
	export real dv_M1 = I0.M1:vds - (I0.M1:vgs - I0.M1:vth)
	export real dv_M2 = M2:vds - (M2:vgs - M2:vth)
	export real dv_M3 = M3:vds - (M3:vgs - M3:vth)
	export real dv_M6 = I0.M6:vds - (I0.M6:vgs - I0.M6:vth)
	export real dv_M4 = abs(I1.M4:vds) - (abs(I1.M4:vgs) - abs(I1.M4:vth))
	export real dv_M5 = abs(I1.M5:vds) - (abs(I1.M5:vgs) - abs(I1.M5:vth))
	export real dv_M7 = abs(I1.M7:vds) - (abs(I1.M7:vgs) - abs(I1.M7:vth))
	export real ov_M0 = I0.M0:vgs - I0.M0:vth
	export real ov_M1 = I0.M1:vgs - I0.M1:vth
	export real ov_M2 = M2:vgs - M2:vth
	export real ov_M3 = M3:vgs - M3:vth
	export real ov_M6 = I0.M6:vgs - I0.M6:vth
	export real ov_M4 = abs(I1.M4:vgs) - abs(I1.M4:vth)
	export real ov_M5 = abs(I1.M5:vgs) - abs(I1.M5:vth)
	export real ov_M7 = abs(I1.M7:vgs) - abs(I1.M7:vth)
}


foreach data {
	foreach vcm_r from swp(start=0.5, stop=0.9, step=0.4){
				run get_dc
	}
}
