

alias measurement get_ac{

// Maintain alphebatical order here.
	run ac
	export real bwout = bw(mag(V(VOUT)),response='low)
	export real gainbwprod = gainBwProd(mag(V(VOUT)))
//	export real gm = gainmargin(V(VOUT))
	export real maxgain = db(max(yval(mag(V(VOUT)))))
	export real pm = phasemargin(V(VOUT))


}

foreach rpnet0239 from swp(start=0, stop=30, step=1){
				run get_ac
}
