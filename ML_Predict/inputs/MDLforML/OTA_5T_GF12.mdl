

alias measurement get_ac{

// Maintain alphebatical order here.
	run ac
	export real bwout = bw(mag(V(n3)),response='low)
	export real gainbwprod = gainBwProd(mag(V(n3)))
//	export real gm = gainmargin(V(n3))
	export real maxgain = db(max(yval(mag(V(n3)))))
	export real pm = phasemargin(V(n3))


}


foreach data{
				run get_ac
}
