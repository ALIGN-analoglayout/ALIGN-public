
alias measurement get_dc {
  run dcOp
	export real dv_M0 = M0:vds - (M0:vgs - M0:vth)
	export real dv_M1 = M1:vds - (M1:vgs - M1:vth)
	export real dv_M2 = M2:vds - (M2:vgs - M2:vth)
	export real dv_M3 = M3:vds - (M3:vgs - M3:vth)
	export real dv_M4 = abs(M4:vds) - (abs(M4:vgs) - abs(M4:vth))
	export real dv_M5 = abs(M5:vds) - (abs(M5:vgs) - abs(M5:vth))
	export real ov_M0 = M0:vgs - M0:vth
	export real ov_M1 = M1:vgs - M1:vth
	export real ov_M2 = M2:vgs - M2:vth
	export real ov_M3 = M3:vgs - M3:vth
	export real ov_M4 = abs(M4:vgs) - abs(M4:vth)
	export real ov_M5 = abs(M5:vgs) - abs(M5:vth)
}


foreach data{
	foreach vcm_r from swp(start=0.565, stop=0.835, step=0.27){
				run get_dc
	}
}
