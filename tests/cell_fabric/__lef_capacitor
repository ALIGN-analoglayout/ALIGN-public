MACRO cap_12f
  ORIGIN 0 0 ;
  FOREIGN cap_12f 0 0 ;
  SIZE 1.716 BY 2.112 ;
  PIN MINUS
    DIRECTION INOUT ;
    USE SIGNAL ;
    PORT
      LAYER M2 ;
        RECT 0.044 0.08 1.644 0.112 ;
    END
  END MINUS
  PIN PLUS
    DIRECTION INOUT ;
    USE SIGNAL ;
    PORT
      LAYER M2 ;
        RECT 0.044 2 1.645 2.032 ;
    END
  END PLUS
  OBS
    LAYER M2 ;
      RECT 0.044 0.192 1.644 1.916 ;
  END
END cap_12f
